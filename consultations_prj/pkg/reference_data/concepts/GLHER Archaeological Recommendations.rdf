    <dcterms:identifier xml:lang="en-us">{"id": "bc3ef28a-cc88-4279-9844-365e4bea01c8", "value": "http://www.archesproject.org/ccb80d1b-e557-40b9-98d3-550ab12bd4e4"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "52a67e15-d204-487c-bbad-270fc0876fa8", "value": "Geoarchaeological Coring"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/1fc86715-4a01-45b8-84c9-00bab02629e6">
    <skos:scopeNote xml:lang="en">{"id": "0f86665b-61da-4650-b92f-8b7c31d8c4da", "value": "Archaeological  geophysical  survey  uses  non-destructive  techniques  to  detect  the  likely  presence  of  archaeological  features.    It  is  usually  used  as  part  of  a  field  evaluation  to  inform  a  planning  decision.    Geophysical  survey  is  largely  restricted  to  undisturbed  greenfield  sites  and  certain  types  of  archaeological  remains.  "}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <skos:prefLabel xml:lang="en">{"id": "ce4b3302-0480-4f06-99c3-ba811701908e", "value": "Geophysical Survey"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en-us">{"id": "028de4b9-4456-4d63-9885-3fdd3ccefd18", "value": "http://www.archesproject.org/1fc86715-4a01-45b8-84c9-00bab02629e6"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/1a93231c-9f74-432c-a04f-b040053a703d">
    <skos:prefLabel xml:lang="en">{"id": "8fc64a50-1429-4fbf-9010-1a3a17d70e20", "value": "Geotechnical Monitoring"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <skos:scopeNote xml:lang="en">{"id": "77a53870-ea0d-4e2c-a1fc-4d64f2031fbf", "value": "Archaeological  monitoring  of  geotechnical  pits  and  boreholes  can  provide  a  cost-effective  means  of  establishing  the  potential  for  archaeological  remains  to  survive  on  previously  developed  land  or  where  deep  deposits  are  anticipated.    It  is  usually  used  as  part  of  a  desk-based  assessment  or  field  evaluation.    "}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "1ef28f79-3001-4876-9791-d1ad36d27f6e", "value": "http://www.archesproject.org/1a93231c-9f74-432c-a04f-b040053a703d"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/bff59299-e25f-42cf-b976-ba26916c19c2">
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <skos:scopeNote xml:lang="en">{"id": "a9c86129-bac9-4918-a706-27f38bb8043f", "value": "As  this  proposal  may  affect  a  heritage  asset  of  architectural,  artistic  or  historic  interest  so  recommend  that  you  seek  the  advice  of  your  conservation  officer. "}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "2bff5fa4-086a-449d-9833-ed519a538a45", "value": "http://www.archesproject.org/bff59299-e25f-42cf-b976-ba26916c19c2"}</dcterms:identifier>
    <skos:prefLabel xml:lang="en">{"id": "439afda4-1fcb-4a50-b7fd-9d7c75e1df3b", "value": "Refer to Conservation Officer"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/1d4e1e46-2c80-4e70-a639-9dffaa496033">
    <skos:scopeNote xml:lang="en">{"id": "4e4e868a-fba3-4f72-8661-3944f92cb4ad", "value": "An  archaeological  watching  brief  involves  observation  of  groundworks  and  investigation  of  features  of  archaeological  interest  which  are  revealed.    A  suitable  working  method  with  contingency  arrangements  for  significant  discoveries  will  need  to  be  agreed.    The  outcome  will  be  a  report  and  archive. "}</skos:scopeNote>
    <dcterms:identifier xml:lang="en-us">{"id": "de79ff4b-5e21-4497-85e7-e12e214a1a4e", "value": "http://www.archesproject.org/1d4e1e46-2c80-4e70-a639-9dffaa496033"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <skos:prefLabel xml:lang="en">{"id": "8ae42810-cd2e-4384-a7db-f34d26d27fa1", "value": "Watching Brief"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/bf5597e0-60ee-4c3a-8349-0c893c2220f9">
    <skos:scopeNote xml:lang="en">{"id": "16e31460-ca62-4864-833e-33b55fd7c127", "value": "In  preparing  a  written  scheme  for  this  site,  the  applicant's  archaeologist  should  consult  English  Heritage's  Regional  Science  Advisor.  "}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <skos:prefLabel xml:lang="en">{"id": "b78d53b0-e34d-42a6-8bac-cd3bbe23f995", "value": "Refer to Science Advisor"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en-us">{"id": "5885a15b-8e05-4a78-a43a-867ac30c53cb", "value": "http://www.archesproject.org/bf5597e0-60ee-4c3a-8349-0c893c2220f9"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/ff99b64f-66e0-4aca-ba5a-edc2b23a54d6">
    <skos:prefLabel xml:lang="en">{"id": "9cbb7d96-fd39-409f-89be-a79fe845c165", "value": "Management Plan"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <dcterms:identifier xml:lang="en-us">{"id": "346450b7-4f36-4c15-af19-be5f6042272c", "value": "http://www.archesproject.org/ff99b64f-66e0-4aca-ba5a-edc2b23a54d6"}</dcterms:identifier>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/1c2658c8-edff-48a5-85bb-bd875f9a97c2">
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <dcterms:identifier xml:lang="en-us">{"id": "01ba19a4-6b71-4da8-adb0-9104156111b6", "value": "http://www.archesproject.org/1c2658c8-edff-48a5-85bb-bd875f9a97c2"}</dcterms:identifier>
    <skos:scopeNote xml:lang="en-us">{"id": "b0fd16d2-cbda-429a-8d70-aa4acf2b9f75", "value": "Where  archaeological  remains  are  to  be  preserved  in-situ  within  a  development  there  will  normally  be  a  requirement  to  provide  details  of  how  this  will  be  achieved.    Typically  this  would  involve  a  design  and  methods  statement  for  groundworks.    Where  particularly  important  or  vulnerable  features  are  to  be  preserved  there  may  also  be  a  requirement  to  monitor  their  condition  and  take  remedial  action  in  the  event  of  decay. "}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "e8bf155e-c31e-47a7-8e17-06eed100fddb", "value": "Preservation in-situ"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/14fd57b3-64ce-435a-bb86-9db33e11c5bd">
    <skos:prefLabel xml:lang="en">{"id": "a5990a5b-fd0e-4e09-89f0-8d3bf20e2eca", "value": "Historic Building Assessment"}</skos:prefLabel>
    <dcterms:identifier xml:lang="en-us">{"id": "c8e877e0-fa88-46f5-a491-22f2c4d9171a", "value": "http://www.archesproject.org/14fd57b3-64ce-435a-bb86-9db33e11c5bd"}</dcterms:identifier>
    <skos:inScheme rdf:resource="http://localhost:8000/e797d64a-d5ea-4c81-939b-19e680a13a77"/>
    <skos:scopeNote xml:lang="en">{"id": "ad703a98-1e72-4902-817c-8a1c989866a2", "value": "Archaeological  building  assessment  is  an  investigation  to  establish  the  character,  history,  dating,  form  and  development  of  an  historic  building  or  structure  to  inform  a  strategy  for  its  conservation,  alteration,  demolition,  repair  or  management. "}</skos:scopeNote>
  </skos:Concept>
