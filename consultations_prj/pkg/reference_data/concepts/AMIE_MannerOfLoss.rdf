    <skos:prefLabel xml:lang="en">{"id": "96f6d433-ba45-4f3b-b16b-54042d0e969f", "value": "Foundered"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "9d99e903-dd20-4bf1-8a30-e7c247417ea6", "value": "A vessel that has sunk at sea: Double-index with background cause were known, e.g. Leaked, Collision."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/dc68ff7c-5783-46bf-9947-7441e67a6de5">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "fea12a06-b5c9-4f8b-a40f-6c56f676bf9d", "value": "Mechanical Failure"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "2f4563b6-e63c-4b1f-89af-74aa4ae7b6e0", "value": "The loss of a craft due to structural or engine failure. In the case of loss resulting from the malfunction of navigational or operational apparatus, use INSTRUMENT FAILURE."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/b68779f2-81b4-46cf-8c10-f9b203006a1d">
    <skos:prefLabel xml:lang="en">{"id": "39d30dea-418d-40ed-9781-a7594c40de03", "value": "Lightning Strike"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "b52164d2-d177-464d-8359-42fcae18754b", "value": "A vessel which has damaged by a lightning strike and subsequently lost."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/78beb915-a35a-4805-b729-82bd0bdc19f7">
    <skos:scopeNote xml:lang="en-us">{"id": "345834c0-6467-4042-9dc7-49594ed079c3", "value": "Use where a section of the vessel has been recovered from where she lies, either at the time or subsequently, leaving behind substantial remains (unlike RECOVERED, where the entire vessel has been removed from the site)."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en-us">{"id": "ff1dd223-da76-4d9c-8fc3-625077d687c7", "value": "Partially Recovered"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/6b6f2ae7-fd58-45f6-a89a-93e846055896">
    <skos:scopeNote xml:lang="en">{"id": "97f3c0b8-c6e1-4594-ac35-4a0f110649dd", "value": "An aircraft which has been damaged beyond repair due to a collision with the ground or sea."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "f8ba313f-a1b5-495f-86bf-5622e63efb2f", "value": "Crashed"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/6e82e23a-0c12-45bc-92e0-8a5cab00bd96">
    <skos:scopeNote xml:lang="en">{"id": "045492ff-fc14-461e-94d3-a1205b0d1a29", "value": "Deliberate collision with another vessel in order to inflict damage."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "be743750-62af-42e0-9d8c-79fcb12dadd0", "value": "Ramming"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/96a4ac44-670e-495b-8a74-751b684d21e7">
    <skos:scopeNote xml:lang="en">{"id": "9e3ab98d-18b8-436b-a985-27caeae00d86", "value": "Loss due to action of free floating or tethered explosive device set to explode on contact with or in proximity to a vessel."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "a7b4d4b7-3013-4b64-891d-84144316c7fc", "value": "Mined"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/9fd36c07-792f-4fe2-94d0-15c6d9a895e7">
    <skos:prefLabel xml:lang="en">{"id": "f50f8d4d-d607-4f55-9db9-34d89a56ddf8", "value": "Explosion"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "37881b10-e92d-4c83-a3a1-07983a0184f5", "value": "A vessel or aircraft lost due to an accidental explosion.  Do not use for losses caused by deliberate action."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/8f8fa050-5807-4200-a538-7e69cedfbbad">
    <skos:scopeNote xml:lang="en">{"id": "87a5b3f7-126a-4758-bda8-a217145c2eed", "value": "A vessel lost due to aerial bombardment."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "7f037ab5-1649-44f0-af39-1efe8cdf0fa0", "value": "Bombed"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/81e776a2-3579-4c0f-96d9-4286a0aa001a">
    <skos:scopeNote xml:lang="en">{"id": "a7ddbb05-1065-4fad-a0a9-5c9ea504db31", "value": "A vessel that is deliberately run or dragged up the beach to save cargo and or crew. Do not use if the vessel is refloated."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "722ac5de-5380-45ec-afaa-d34d87c9d4fe", "value": "Beached"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/7295b583-c1e4-4bd8-a926-ddaa73aa3d8b">
    <skos:scopeNote xml:lang="en">{"id": "5557f02a-e659-4427-9ee8-806fa19a135f", "value": "Ignition from within rather than through external forces; frequently associated with cargoes of organic material such as manure, owing to an internal rise in temperature of the cargo itself."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "19d8a6df-65e4-4fac-a778-1e160d5907d9", "value": "Spontaneous Combustion"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/cbc305c0-db35-4366-a65a-b6f0c921f6df">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "2f68965a-73ec-4e94-b1a8-8be078626f42", "value": "Wrecked"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "0abb8440-c61e-497d-84c5-2039c5569044", "value": "A vessel which comes ashore and breaks up immediately due to severe weather."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/bbed87b1-2e93-4d98-9f62-f650e09c3f87">
    <skos:prefLabel xml:lang="en">{"id": "5539a411-a96c-4a28-81c3-2a80de820bb6", "value": "Broken Up"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "362d9e91-a52a-4501-812c-3de466687ae0", "value": "A vessel which has been dismantled for the purpose of removal from the shore after stranding."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/8eaddf48-0ce1-49c7-bd51-93dfe1ef324f">
    <skos:prefLabel xml:lang="en">{"id": "d18d05a1-b5da-461b-afa1-4a652e5ad56b", "value": "Burnt"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "5ae98a20-2729-4163-b4a0-31fa7ef7e4d3", "value": "Loss of a vessel or aircraft primarily due to fire."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/332926f7-753b-430a-b53e-924c7430045c">
    <skos:prefLabel xml:lang="en">{"id": "c8722ffd-996a-41d1-b733-b585930c7326", "value": "Laid Up"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "f3f091e0-c048-4b06-abcd-5e7931645978", "value": "A vessel withdrawn from active use and awaiting disposal or reuse.  Vessel subsequently becomes derelict."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/f41a5164-5ca6-41b2-9871-e84cef342961">
    <skos:scopeNote xml:lang="en">{"id": "0dbad769-d142-4bd6-a873-0dec10f58635", "value": "Loss of a vessel due to bombardment from ship, shore or submarine. For Aircraft use Shot Down."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "7e3c9715-fd5b-416b-8bc0-4b3510c8423e", "value": "Gun Action"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/7410475f-e962-4c71-ba17-9798767065a8">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "948f1abb-3406-4ba5-8021-18a5624a74c1", "value": "Dispersed"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "ee3d8ebd-bcf8-4078-8c4e-594e5a9d8dda", "value": "For use when a vessel, already lost through other causes, is dismantled or dispersed by means of explosives to render it harmless to other shipping."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/95ac4c90-0031-4514-af02-40c6254cf600">
    <skos:prefLabel xml:lang="en">{"id": "60d4fb77-6da2-4b40-88b6-240e9e03ffb1", "value": "Back Broken"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "9a5bc0a7-70d9-4147-af6e-ae0cf4eba32e", "value": "Use when the vessel splits in two due to stresses on her frame where she lies unsupported either amidships or at the ends, typically after stranding, although this may also happen in similar conditions on the seabed after foundering."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/d1e6fe4c-2f31-46d6-8fbc-224f5eac94f8">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "4cd12513-1798-44a6-85d9-e24acaa959d0", "value": "Use for 'constructive total loss' (CTL) or where a vessel is abandoned because she is beyond repair or has become so after abandonment, which may include acts of vandalism."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "fa48dbcc-73dd-45f0-b6ba-8b53168a6dc6", "value": "Beyond Repair"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/da8e0699-30ee-41ff-b3cf-e4bc8dea9a4b">
    <skos:scopeNote xml:lang="en">{"id": "3fc3821d-1696-45c3-90f1-5d81c1b20025", "value": "Loss of a vessel or aircraft due to an explosion caused by its own armament or cargo."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "ef9d2e66-2f02-47c0-bba0-c544b33638e0", "value": "Internal Explosion"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/009cab5b-98f9-4899-bc05-92b44e33c0f4">
    <skos:prefLabel xml:lang="en">{"id": "749be55f-fb39-4ec8-9f4d-953ee84498c2", "value": "Vandalism"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "e19b0b35-d313-4c0e-a92f-035872b7619d", "value": "Acts of vandalism, or deliberate damage by owner or other parties, which result in the 'Constructive Total Loss' (CTL) of vessel, or contributes to CTL of an abandoned vessel."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/9f2a821d-be21-4e87-8b38-05c86b93114a">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "9ec30477-1bb7-464a-982e-0326442b33a8", "value": "Abandoned"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "42cdb4f2-1a98-4624-a13d-5e14d4c27909", "value": "Refers to the abandonment of a vessel at sea due to its becoming unseaworthy.  Do not use for vessels abandoned having run aground. For aircraft use DITCHED."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/550d632f-53b9-48a4-9e38-d5d408586f60">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "11ccc780-45a2-4b1e-b4c0-95d20d8b790c", "value": "Bilged"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "0f1c6a6f-5254-40e5-8ea5-991a305833f9", "value": "Stoving in or fracturing of lowest part of vessel's hull."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/33748aa7-2767-496d-8c76-3192f7b09e36">
    <skos:prefLabel xml:lang="en">{"id": "4385333d-a97c-47f8-886e-78c149aee534", "value": "Leaked"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "8503aa96-7060-418f-9643-560654f7a3fa", "value": "Ingress of water through damage or defects in the hull. Double-index with any cause, were known, e.g. Collision, and consequence, were known, e.g. Beached, Foundered."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/0da6c1b8-c689-46f4-8415-b14fe7b54462">
    <skos:scopeNote xml:lang="en">{"id": "94b45da2-36f6-40e3-8ba7-b0f24aee06d3", "value": "Use where an entire, wrecked or incapacitated, craft has been recovered or salvaged, possibly leaving debris or other evidence of its presence at the wreck or crash site. If part of the wreck remains on the seabed use PARTIALLY RECOVERED."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "82f98fa4-6fee-4f5a-842f-38ab374d2b1c", "value": "Recovered"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/2de8bf5c-788f-4634-b5b9-bcf07d3b3f69">
    <skos:scopeNote xml:lang="en">{"id": "e67d3d81-a33a-41f6-88b4-4bef2e63a38e", "value": "Loss of a craft, especially aircraft, resulting from the malfunction of registering or measuring apparatus vital to its navigation or operation. In the case of structural or engine failure use Mechanical Failure."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "c96fd4ba-72b6-4615-b085-adaf10b41e44", "value": "Instrument Failure"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/ce998130-870a-426b-8ede-9040d00a40ac">
    <skos:prefLabel xml:lang="en">{"id": "2ede1eb6-5d95-4952-9e91-d3796ad2e72c", "value": "Collision"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "377468e0-b535-4c14-b052-dc3276da8b05", "value": "Loss of a vessel or aircraft through accidental contact with another or a structure. Do not use when contact is with natural feature or rocky shore."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/278a9a76-4646-437b-8c30-12e62fd766a4">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "20844dd6-a63d-4709-a514-4479606400ea", "value": "Lost"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "bd0ded4e-c5c7-41c6-85db-4094697f3ebe", "value": "A vessel or aircraft lost at sea, cause unknown."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/d6e2ba3a-3523-4336-be84-073516b79db6">
    <skos:prefLabel xml:lang="en">{"id": "53c26b91-29be-4636-9fcc-8af64a620bd5", "value": "Distressed"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "fda6232a-8af3-428c-8400-dc97c6072a98", "value": "Damaged, usually by weather or warfare, to such an extent that the vessel is in severe or imminent danger of being lost; i.e. a pre-existing condition of the vessel which contributes to the final wrecking event."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/a111a4f8-ed30-41e8-8b58-6f45cce9707c">
    <skos:scopeNote xml:lang="en">{"id": "8b6b71de-a7c8-464c-b3e1-cc30d2ad0800", "value": "A vessel which has been deliberately scuttled using explosive charges."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "66ca6eca-a200-4326-aac0-e17127557350", "value": "Scuttling Charges"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/f26e608d-0d3f-4e3d-bcaf-2e8891233a4f">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "f97bac8a-965a-4762-bc43-518153e2e8d4", "value": "Scuttled"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "40d9bc41-460a-40a3-be9c-52073612bbff", "value": "Deliberate sinking of a vessel, e.g. to prevent capture or to act as a blockship."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/8f38defd-f2bb-4dcb-9158-12fb63ef55d9">
    <skos:scopeNote xml:lang="en">{"id": "09670895-8bea-452a-b6f9-d454b1d29b04", "value": "Use only for fragmentary wreckage washed up at the time of loss or subsequently: but not for complete hulls for which Stranded or another appropriate manner of loss should be used."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "f27485de-2c02-426f-a9da-86747ce6c3ff", "value": "Washed Up"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/a515cd84-2903-439c-b4c3-4de73035d85e">
    <skos:scopeNote xml:lang="en">{"id": "3ad4938c-e876-4d9f-948c-c7b1798b66c8", "value": "A vessel which runs aground and does not get off."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "7d74065f-9167-4def-b2c4-744098b3de2b", "value": "Stranded"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/8ad961f3-ff8e-406c-a6d9-e2e7d63fdf1d">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "ea1be062-d586-42c8-bf25-2534e48414c9", "value": "Ditched"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "038258f0-5252-4dc5-8587-b52f369b50a9", "value": "An aircraft which has been forced to land on the sea in an emergency."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/32c106a1-a66c-4666-815e-44ab5bb0a8a6">
    <skos:scopeNote xml:lang="en">{"id": "689d21bb-ac61-4e07-aaa7-f46e569d9c11", "value": "Judged as irreparable or no longer seaworthy following a wrecking incident (from which the vessel may have been recovered) or a difficult voyage: a term with legal force indicating that the vessel is unfit for service."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "dab08a65-d86b-4059-be8e-cee9bd421e3e", "value": "Condemned"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/5c964d37-ba0b-4e94-877f-fcbb713b5f33">
    <skos:prefLabel xml:lang="en">{"id": "28263de5-0e69-483b-9b1e-b7ebaffda704", "value": "Grounded"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "e3211a4a-bc35-423e-9e3a-c812bdf8ff71", "value": "Loss of a vessel due to running aground and subsequently refloating before final loss."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/490a5e68-c63d-4ce5-8d4c-0b8b0b7a94db">
    <skos:scopeNote xml:lang="en">{"id": "5db1dca8-b219-49fe-9476-2879c638c34a", "value": "Loss due to action of self propelled submerged missile."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "7b3dad30-47e5-4eca-878f-e1b73ee40155", "value": "Torpedoed"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/5dff867e-1c97-407a-b7a7-814feaf8b549">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "70cc97ec-3e63-4e04-9a72-94e2dd2a137b", "value": "Capsized"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "ae80764e-0704-4e7e-a753-b34fb2259948", "value": "A vessel lost having overturned due to severe weather or shifting cargo."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/7be0049c-c389-4435-97dc-045875c80b08">
    <skos:scopeNote xml:lang="en">{"id": "5d2d6193-09ed-48ba-8b1b-e79bfecd6393", "value": "Loss of vessel due to deployment of explosive charges set to detonate at a predetermined depth."}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "50a64fbc-fbb3-47be-aafd-d86896a1e8c5", "value": "Depth Charged"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/40c736b9-72e9-4e25-959f-01ca978946d0">
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "15cb190b-865f-414a-b6ba-12cad9e7c39c", "value": "Used in Construction"}</skos:prefLabel>
    <skos:scopeNote xml:lang="en">{"id": "d9e96514-2ad3-4c6b-9398-74d55719f153", "value": "Reuse of timbers or hull of a vessel in construction. For example sinking of hulks as harbour foundations or recycling of timbers in domestic contexts."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/315035ef-8e31-4e9f-ab55-2475f009a972">
    <skos:prefLabel xml:lang="en">{"id": "c6a5a0a6-7bba-4a31-9a79-3a0e73a5645e", "value": "Enemy Action"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:scopeNote xml:lang="en">{"id": "dd83c41b-658f-435d-bad2-fc2207c96f01", "value": "Loss due to cumulative damage sustained during acts of warfare, capture, piracy, privateering etc. Use in instances where a more specific manner of loss (e.g. Scuttled, Gun Action) cannot be assigned."}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/eb4e1803-84dc-4193-90c8-6890f79b82f3">
    <skos:scopeNote xml:lang="en">{"id": "b06de45a-ed2f-443b-af45-ab12c7e48a5c", "value": "An aircraft lost due to offensive action. Includes air-to-air and ground-to-air combat. For Vessels use Gun Action."}</skos:scopeNote>
    <skos:inScheme rdf:resource="http://localhost:8000/1a292f5a-0e82-496a-aede-53f38b1bbc1d"/>
    <skos:prefLabel xml:lang="en">{"id": "d2c60b32-3c6b-46cb-95cf-c8d1337a7be7", "value": "Shot Down"}</skos:prefLabel>
  </skos:Concept>
</rdf:RDF>
